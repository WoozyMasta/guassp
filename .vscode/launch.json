{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "App",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "app.py",
        "FLASK_ENV": "development"
      },
      "args": [
        "run",
        "--no-debugger"
      ],
      "jinja": true
    },
    {
      "name": "Worker",
      "type": "python",
      "request": "launch",
      "program": "worker.py",
      "console": "integratedTerminal",
      "env": {
        "LOG_LEVEL": "info"
      }
    }
  ]
}
